{
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "baseUrl": "src/",
    "paths":{
      "@ui/*":["shared/UIkit/*"],
      "@ui":["shared/UIkit"],
    }
  }
}